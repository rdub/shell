# environment variables
. ~/.shell/environment

# color definitions
. ~/.shell/colors

#aliases
. ~/.shell/aliases

# git functions
. ~/.shell/git_utils

# p4 functions
. ~/.shell/p4_utils

# e environment manager
if [ -d ~/.e ]; then
	eval "$(~/.e/e.py init)"
fi

# prompt
. ~/.shell/prompt

# utility functions
. ~/.shell/utilities

# history control
. ~/.shell/history

# Raise the open file limit to 1024
ulimit -S -n 1024

# Bash Completion
if [ -f $(brew --prefix)/etc/bash_completion ]; then
	. $(brew --prefix)/etc/bash_completion
fi
